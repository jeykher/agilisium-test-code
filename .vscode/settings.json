{
    // Activitus Bar Configuration
    "activitusbar.views": [
        {
            "name": "explorer",
            "codicon": "explorer-view-icon"
        },
        {
            "name": "search",
            "codicon": "search-view-icon"
        },
        {
            "name": "scm",
            "codicon": "source-control-view-icon"
        },
        {
            "name": "debug",
            "codicon": "run-view-icon"
        },
        {
            "name": "extensions",
            "codicon": "extensions-view-icon"
        },
        {
            "name": "extension.project-manager",
            "codicon": "folder"
        },
        {
            "name": "extension.thunder-client",
            "codicon": "symbol-event"
        }
    ],
    // Bracket pair coloraized configuration
    "brackets-pair-colorized-2.colors": [
        "#fafafa",
        "#9f51b6",
        "#f7c244",
        "#f07850",
        "#9cdd29",
        "#0098fa"
    ],
    // Eslint
    "eslint.validate": [
        "javascript",
        "javascriptreact",
        "typescript",
        "typescriptreact"
    ],
    // Material Icon theme
    "material-icon-theme.folders.associations": {
        ".turbo": "tasks",
        "brands": "proto",
        "buttons": "include",
        "cards": "queue",
        "development": "update",
        "domain": "resource",
        "dtos": "coverage",
        "entities": "ngrx-entities",
        "files": "archive",
        "globals": "global",
        "info": "lua",
        "infrastructure": "buildkite",
        "mono": "docs",
        "navbars": "app",
        "seed": "generator",
        "single": "docs",
        "thirdparty": "container",
        "tsconfig": "typescript",
        "ui": "delta",
        // Special
        "artist": "svg",
        "react-reactstrap-ts-ui": "shader",
        "song": "audio",
        "songs": "views"
    },
    "material-icon-theme.files.associations": {
        "*.adapter.ts": "scala",
        "*.class.ts": "matlab",
        "*.context.jsx": "credits",
        "*.context.tsx": "credits",
        "*.controller.ts": "rc",
        "*.data.js": "database",
        "*.data.ts": "database",
        "*.decorator.ts": "codecov",
        "*.dto.ts": "robot",
        "*.entity.ts": "sketch",
        "*.function.ts": "sas",
        "*.hook.jsx": "svgr",
        "*.hook.tsx": "svgr",
        "*.interface.ts": "less",
        "*.layout.jsx": "mjml",
        "*.layout.tsx": "mjml",
        "*.mock.ts": "mocha",
        "*.module.ts": "makefile",
        "*.page.jsx": "contributing",
        "*.page.tsx": "contributing",
        "*.provider.js": "smarty",
        "*.provider.jsx": "smarty",
        "*.provider.ts": "smarty",
        "*.provider.tsx": "smarty",
        "*.service.ts": "puppet",
        "*.theme.js": "renovate",
        "*.theme.ts": "renovate",
        "*.type.ts": "lua",
        "app.jsx": "azure",
        "app.tsx": "azure",
        "extensions.json": "vscode",
        "fonts.scss": "fortran",
        "globals.scss": "blink",
        "info.md": "shaderlab",
        "launch.json": "vscode",
        "layout.tsx": "mjml",
        "main.js": "parcel",
        "main.jsx": "parcel",
        "main.ts": "parcel",
        "main.tsx": "parcel",
        "page.tsx": "contributing",
        "settings.json": "vscode",
        "tasks.json": "vscode",
        "themes.js": "julia",
        "themes.ts": "julia"
    },
    // Peacok Configuration
    "peacock.color": "#6c076b",
    // SynthWave 84 Vscode Theme Configuration
    "synthwave84.brightness": 0.45,
    "synthwave84.disableGlow": false,
    // Terminal integrated
    "terminal.integrated.fontFamily": "Hack Nerd Font Mono",
    "terminal.integrated.fontSize": 13,
    // Vscode Editor
    "editor.codeActionsOnSave": {
        "source.fixAll.eslint": true,
    },
    "editor.fontFamily": "'Victor Mono', Consolas, 'Courier New', monospace",
    "editor.fontLigatures": true,
    "editor.fontSize": 14,
    "editor.fontWeight": "bold",
    "editor.formatOnSave": true,
    "editor.minimap.enabled": false,
    // Vscode Features
    "editor.stickyTabStops": true,
    "editor.stickyScroll.enabled": true,
    // Vscode Workbench
    "workbench.activityBar.visible": true,
    "workbench.colorCustomizations": {
        "activityBar.activeBackground": "#9c0a9a",
        "activityBar.background": "#9c0a9a",
        "activityBar.foreground": "#e7e7e7",
        "activityBar.inactiveForeground": "#e7e7e799",
        "activityBarBadge.background": "#94960a",
        "activityBarBadge.foreground": "#15202b",
        "commandCenter.border": "#e7e7e799",
        "sash.hoverBorder": "#9c0a9a",
        "statusBar.background": "#6c076b",
        "statusBar.foreground": "#e7e7e7",
        "statusBarItem.hoverBackground": "#9c0a9a",
        "statusBarItem.remoteBackground": "#6c076b",
        "statusBarItem.remoteForeground": "#e7e7e7",
        "titleBar.activeBackground": "#6c076b",
        "titleBar.activeForeground": "#e7e7e7",
        "titleBar.inactiveBackground": "#6c076b99",
        "titleBar.inactiveForeground": "#e7e7e799"
    }
}